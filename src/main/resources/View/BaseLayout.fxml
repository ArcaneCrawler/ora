                          </columns>
                        </TableView>
                         <Button layoutX="253.0" layoutY="366.0" mnemonicParsing="false" onAction="#onClickAdd" text="Добавить" />
                        <Button layoutX="384.0" layoutY="366.0" mnemonicParsing="false" onAction="#onClickDelete" text="Удалить" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Населённые пункты">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="locacityTableView" layoutX="16.0" layoutY="4.0" prefHeight="348.0" prefWidth="578.0">
                          <columns>
                            <TableColumn fx:id="idLocacityColumn" prefWidth="59.0" text="ID" />
                            <TableColumn fx:id="nameLocacityColumn" prefWidth="70.0" text="Название" />
