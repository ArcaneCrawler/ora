                              <TableColumn fx:id="chanceVirusColumn" prefWidth="38.0" text="P" />
                              <TableColumn fx:id="evolVirusColumn" prefWidth="92.0" text="К.С. развития" />
                              <TableColumn fx:id="enduranceVirusColumn" prefWidth="94.0" text="Уcтойчивость" />
