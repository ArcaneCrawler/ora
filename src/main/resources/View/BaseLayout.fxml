                              <TableColumn fx:id="curedJournalColumn" prefWidth="83.0" text="Вылеченные" />
                              <TableColumn fx:id="suspectedJournalColumn" prefWidth="67.0" text="Здоровые" />
                           </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
